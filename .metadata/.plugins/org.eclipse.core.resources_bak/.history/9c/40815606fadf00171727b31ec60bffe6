package Game;

import java.awt.Graphics2D;

public class GUI {

	private Main main;
	
	public GUI (Main main) {
		this.main = main;
	}
	
	public void tick() {
		
	}

	public void render(Graphics2D g) {
		for(int i = 0; i < main.getPlayerList().size(); i++) {
			g.setColor(main.getPlayerList().get(i).getColor());
			g.drawString("Player " + i + ": " + , x, y);
		}
	}
	
}